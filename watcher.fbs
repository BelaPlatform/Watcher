namespace Bela.Watcher;

table DataMsg {
  var_id:uint16;
  type_id:string;
  timestamp:uint64;
  payload_data_size:uint32;
  payload_timestamp_size:uint32 = 0;
  alignment:uint8 = 4;
}

root_type DataMsg;

table FileHeader {
  what:string;
  var_name:string;
  pid:uint64;
  ptr:uint64;
}

root_type FileHeader;
